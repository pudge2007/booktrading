<div class="container">  
  <div class="row">
    <div class="col-sm-6 col-sm-offset-3 col-xs-12">
      <div class="page-new">
        <h1 class='text-center'>Update Profile</h1>
        <form role="form" ng-submit="updateProfile()">
          <div class="form-group">
            <label for="city">City</label>
            <input type="text" ng-model="city" class="form-control" id='city' />
          </div>
          <div class="form-group">
            <label  for="state">State</label>
              <input type="text" ng-model="state" class="form-control" id='state' />
          </div>    
          <div class="row">
            <div class="col-xs-6">
              <button class="btn btn-vote pull-left" type="submit">save</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>